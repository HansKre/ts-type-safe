{
  "name": "ts-type-safe",
  "version": "1.0.12",
  "description": "Utilities for a more type-safe TypeScript",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "build:doc": "jsdoc2md --configure ./jsdoc2md.json --template README.hbs --no-cache --files ./src/**/*.ts > README.md",
    "build:doc:inspect": "jsdoc2md --json --configure ./jsdoc2md.json --no-cache --files ./src/**/*.ts > jsdoc2md.out.json",
    "prepare": "npm run test && npm run build && npm run build:doc",
    "test": "jest",
    "test:w": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:HansKre/ts-type-safe.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "types",
    "type-safe",
    "typeguards",
    "type-guards"
  ],
  "author": "HansKre",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-to-markdown": "^8.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
